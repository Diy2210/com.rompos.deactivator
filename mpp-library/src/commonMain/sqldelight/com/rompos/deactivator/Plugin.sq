CREATE TABLE Plugins (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  title TEXT NOT NULL,
  url TEXT NOT NULL,
  token TEXT NOT NULL
);

selectAll:
SELECT *
FROM Plugins;

selectPlugin:
SELECT * FROM Plugins WHERE id = ?;

insertPlugin:
INSERT INTO Plugins(title, url, token)
VALUES (?, ?, ?);

updatePlugin:
UPDATE Plugins
SET title = ?, url = ?, token = ? WHERE id = ?;

deletePlugin:
DELETE FROM Plugins WHERE id = ?;

deleteAllPlugins:
DELETE
FROM Plugins;

